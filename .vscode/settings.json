{
    "php.suggest.basic": false,
    "php-docblocker.useShortNames": true,
    "phpcbf.onsave": true,
    "phpcbf.standard": "./vendor/aereus/codequality/phpcs.xml",
    "phpcs.standard": "./vendor/aereus/codequality/phpcs.xml",
    "phpcs.showSources": true,
    "phpcs.trace.server": "messages",
    "phpcs.executablePath": "./vendor/bin/phpcs",
    "phpmd.command": "./vendor/bin/phpmd",
    "phpmd.rules": "./vendor/aereus/codequality/phpmd.xml",
    "editor.renderControlCharacters": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.css-language-features",
    "[php]": {
      "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}
  
